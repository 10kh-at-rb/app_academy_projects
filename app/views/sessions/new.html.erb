<h1>Log in plz</h1>

<form action="<%= session_url %>" method="post">
  <label for="user_username">username</label>
  <input
    type="text"
    name="user[username]"
    value="<%= @user.username %>"
    id="user_username">
  <br />
  <label for="user_password">password</label>
  <input
    type="password"
    name="user[password]"
    id="user_password">
  <input type="submit" name="Log In">
</form>
