<h2>Sign up!</h2>

<form class="" action="<%= users_url %>" method="post">
  <%= auth_token %>
  <label for="user_username">Username</label>
  <input
    type="text"
    name="user[username]"
    id="user_username">

  <label for="user_password">Password</label>
  <input
    type="password"
    name="user[password]"
    id="user_password">

  <input type="submit" name="Create" value="Sign up">
</form>
