<h2>All Subs</h2>
<table>
  <tr>
    <th>
      Title
    </th>
    <th>
      Description
    </th>
    <th>
      Moderator
    </th>
  </tr>
  <% @subs.each do |sub| %>
  <tr>
    <td>
      <a href="<%= sub_url(sub) %>"><%= sub.title %></a>
    </td>
    <td>
      <%= sub.description %>
    </td>
    <td>
      <a href="<%= user_url(sub.moderator_id) %>"><%= sub.moderator.username %></a>
    </td>
  </tr>
  <% end %>
</table>
